<%- include('header')%>

    <div class="box" id="heading">
        <h1><%=kindOfDay%></h1>
    </div>
   <div class="box">
   <% newActivity.forEach(function(task){%>
        <div class="item">
            <input type="checkbox">
       <p><%=task.name%></p>
        </div>
   <% })%>
    <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
        <button type="submit" name="lists" value="<%-kindOfDay%>">+</button>
    </form>
</div>
<%- include('footer')%>
   
  